<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hjcrm.system.dao.DealrecordDao">
    <insert id="addDealrecord" parameterType="dealrecord">
        insert into hj_dealrecord(dealrecordId,userid,resourceId,courseid,create_time,subjectid)
        values (null,#{userid},#{resourceId},#{courseid},#{create_time},#{subjectid})
    </insert>
    
</mapper>
